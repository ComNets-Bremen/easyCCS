{
  "name": "ccs-client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config src/proxy.conf.json",
    "start-local": "ng serve --configuration servelocal",
    "build": "ng build",
    "build-prod": "ng build --configuration production",
    "test": "ng test",
    "lint": "ng lint",
